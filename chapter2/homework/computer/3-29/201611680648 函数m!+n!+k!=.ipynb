    "def jc(n):\n",
    "    i=0\n",
    "    p=1\n",
    "    while i<n:\n",
    "        i+=1\n",
    "        p*=i\n",
    "    return p\n",
    "m=int(input('请输入整数m的值'))\n",
    "n=int(input('请输入整数n的值'))\n",
    "k=int(input('请输入整数k的值'))\n",
    "print('m!+n!+k!=',jc(m)+jc(n)+jc(k))"
   ]
  }
