    "def sum(n):\n",
    "    pi = 0\n",
    "    t = 1\n",
    "    i = 1\n",
    "    s = 1\n",
    "    while i < n:\n",
    "        pi = pi + t\n",
    "        i = i + 2\n",
    "        s = -s\n",
    "        t = s / i\n",
    "    return pi\n",
    "n1 = 1000\n",
    "print (4 * sum(n1))\n",
    "n2 = 100000\n",
    "print(4 * sum(n2))\n",
