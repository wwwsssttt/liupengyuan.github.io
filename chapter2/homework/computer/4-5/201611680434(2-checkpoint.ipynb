   "source": [
    "import random\n",
    "n = int(input('请输入一个大于0的整数，回车结束。'))\n",
    "a = random.randint(1, 9)\n",
    "i = 0\n",
    "s = 0\n",
    "while i < n:\n",
    "    i += 1\n",
    "    b = ((10**i - 1)/9) * a\n",
    "    s += b\n",
    "    \n",
    "print(s)\n"
   ]
  }
