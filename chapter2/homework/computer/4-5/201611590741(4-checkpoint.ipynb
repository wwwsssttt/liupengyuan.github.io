   "source": [
    "import random, math\n",
    "total = 0\n",
    "i = 0\n",
    "n = int(input('请输入一个整数，按回车键结束'))\n",
    "m = int(input('请输入一个上界，按回车键结束'))\n",
    "k = int(input('请输入一个下界，按回车键结束'))\n",
    "while i <= n:\n",
    "    number = random.randint(m, k)\n",
    "    total = total + number\n",
    "    i += i+1\n",
    "print(math.sqrt(total / n))\n"
   ]
  }
