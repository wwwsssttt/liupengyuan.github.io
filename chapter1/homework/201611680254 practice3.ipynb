    "n=int(input('请输入你想输入的数字'))\n",
    "c=int(input('请再次输入你想输入的数字'))\n",
    "m=0\n",
    "s=n+m+c\n",
    "p=n*c\n",
    "\n",
    "while s>=m and p>=m**2:\n",
    "    s=s+m\n",
    "    p=p*m\n",
    "    m=int(input('请再次输入你想输入的数字'))\n",
    "else:\n",
    "    print('结束，谢谢输入')"
